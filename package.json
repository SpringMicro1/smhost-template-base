    "astro": "astro",
    "enable": "node enable-page.js --enable",
    "disable": "node enable-page.js --disable",
    "prep": "sh firstdeployprep.sh",
    "stop": "pm2 stop ss"
  },
  "dependencies": {
    "@astrojs/image": "^0.14.1",
    "@astrojs/mdx": "^0.16.0",
    "@astrojs/sitemap": "^1.0.1",
    "@astrojs/tailwind": "^3.0.1",
    "@fontsource/inter": "^4.5.15",
    "astro": "^2.0.11",
    "astro-feather-icons": "^1.0.2",
    "astro-icon": "^0.8.0",
    "astro-navbar": "^1.1.5",
    "astro-seo": "^0.7.0",
    "tailwindcss": "^3.2.4"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "playwright": "^1.33.0",
    "pm2": "^5.3.0",
    "sharp": "^0.31.3",
    "vercel": "^29.1.1"
  }
