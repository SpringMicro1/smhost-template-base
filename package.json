{
  "name": "smhost-template-base",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "buildlocal": "astro build && pm2 --name ss serve ./dist && node screenshot.js ./public && pm2 stop ss",
    "build": "astro build && pm2 --name ss serve ./dist && node screenshot.js ./dist && pm2 stop ss && vercel domains ls",
